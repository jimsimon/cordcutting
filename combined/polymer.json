{
  "entrypoint": "static/index.html",
  "shell": "static/src/cordcutting-app.html",
  "fragments": [
    "static/src/bundle-wizard.html",
    "static/src/bundle-wizard-results.html",
    "static/src/page-not-found.html"
  ],
  "sources": [
    "static/src/**/*",
    "static/images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "static/manifest.json",
    "static/bower_components/webcomponentsjs/*.js"
  ],
  "lint": {
    "rules": [
      "polymer-2"
    ]
  },
  "builds": [
    {
      "name": "es6-unbundled",
      "preset": "es6-unbundled",
      "basePath": true,
      "browserCapabilities": [
        "es2015",
        "push"
      ]
    },
    {
      "name": "es6-bundled",
      "preset": "es6-bundled",
      "basePath": true,
      "browserCapabilities": [
        "es2015"
      ]
    },
    {
      "name": "es5-bundled",
      "preset": "es5-bundled",
      "basePath": true
    }
  ]
}
